@inproceedings{Schmidt00,
	Abstract = {CSCW at large seems to be pursuing two diverging strategies: on one 
hand a strategy aiming at coordination technologies that reduce the complexity of 
coordinating cooperative activities by regulating the coordinative interactions, and 
on the other hand a strategy that aims at radically flexible means of interaction 
which do not regulate interaction but rather leave it to the users to cope with the 
complexity of coordinating their activities. As both strategies reflect genuine 
requirements, we need to address the issue of how the gap can be bridged, that is, 
how the two strategies can be integrated conceptually. In addressing this problem, 
the paper discusses two general modalities of articulation work --- ad hoc alignment 
and improvisation on the basis of mutual awareness versus coordination in terms of 
a predefined flow of work --- and argues that these modalities are seamlessly 
meshed and blended in the course of real world cooperative activities. On the basis 
of this discussion the paper outlines an approach which may help CSCW research to 
bridge this gap.},
	Address = {Sophia Antipolis, France},
	Author = {Schmidt, K. and Simone, C.},
	Booktitle = {Proceedings of COOP2000: The Fourth International Conference on the Design of Cooperative Systems},
	Date-Added = {2007-08-29 10:56:36 +0200},
	Date-Modified = {2009-08-03 22:41:25 +0200},
	Keywords = {Articulation Work},
	Publisher = {INRIA},
	Read = {Yes},
	Title = {{Mind the Gap!: Towards a unified view of CSCW}},
	Year = {2000},
	Bdsk-File-1 = {}}

